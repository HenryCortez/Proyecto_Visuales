	<queryString>
		<![CDATA[SELECT
  u.SUE_USU,
  FLOOR((SELECT SUM(m2.VAL_MUL) FROM multas m2 WHERE m2.ID_ASI = i.ID_ASI OR m2.ID_SAL = s.ID_SAL)) AS MULTA,
FLOOR((SELECT SUM(m2.VAL_MUL) / 0.25 FROM multas m2 WHERE m2.ID_ASI = i.ID_ASI OR m2.ID_SAL = s.ID_SAL)) AS Min_Ret,
 (800-FLOOR((SELECT SUM(m2.VAL_MUL) / 0.25 FROM multas m2 WHERE m2.ID_ASI = i.ID_ASI OR m2.ID_SAL = s.ID_SAL))*0.25) AS PAGA
FROM
  ingresos i, salidas s, multas m, usuarios u
WHERE
  i.ID_USU = s.ID_USU_SAL
  AND i.ID_USU = $P{id_user}
  AND i.FEC_HOR_ING LIKE CONCAT($P{fechaC}, "%")
  AND s.FEC_HOR_SAL LIKE CONCAT($P{fechaC}, "%")
  AND (m.ID_ASI = i.ID_ASI or m.ID_SAL = s.ID_SAL)
limit 1;]]>
	</queryString>
